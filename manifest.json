{
  "manifest_version": 3,
  "name": "CodePusher",
  "version": "1.0",
  "permissions": ["identity", "storage", "scripting", "tabs"],
  "host_permissions": [
    "https://api.github.com/",
    "http://localhost:3000/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  }
}
