{
  "manifest_version": 3,
  "name": "LeetCode GitHub Pusher",
  "version": "1.0",
  "description": "Push your LeetCode code directly to GitHub from this extension.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "permissions": ["storage", "scripting", "activeTab"],
    "oauth2": {
    "client_id": "YOUR_CLIENT_ID_FROM_GITHUB",
    "scopes": ["repo", "user"]
  },
  "host_permissions": ["https://api.github.com/*"]
}
